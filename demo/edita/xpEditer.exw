function menu_handler(gdx menu, integer id, bool bHighlight)
--function menu_handler(gdx menu, integer id)
    string s = gMenuGetAttribute(menu,id,"TITLE"),
           h = iff(bHighlight?" (highlight)":
--             iff(not find(id,{NORMALC,DOTTEDC,FILLEDC})?"":
               iff(not find(id,{SHOWFMT,PANEL})?"":
               iff(gMenuGetAttribute(menu,id,"CHECKED")?" (Checked)":" (unchecked)")))
--DEV as per btn_click, just invoke the keyboard handler? (at least for those we can, 
--                                                      ie those with \t in the title)
    printf(1,"%s%s (menu=%d, id=%d)\n",{s,h,menu,id})
--  if id=CLOSE and not bHighlight then
----        integer mitem = EDIT_MENU
----        integer mitem = OPEN
--      integer mitem = PASTE
--      bool bActive = gMenuGetAttribute(menu,mitem,"ACTIVE")
----?{"CLOSE:",bActive,CLOSE,EDIT_MENU}
--      gMenuSetAttribute(menu,mitem,"ACTIVE",not bActive)
--  end if
    return iff(id=EXIT and not bHighlight?XPG_CLOSE:XPG_DEFAULT)
end function

function toolbar_item(atom action, string image, string tip, integer active, id=0)
    gdx item = gButton(NULL, action, "CANFOCUS=NO, FLAT=YES")
--  gdx item = gButton("", action, "CANFOCUS=NO, FLAT=YES")
--  gdx item = gButton(tip, action, "CANFOCUS=NO, FLAT=YES")
--  gdx item = gButton(" ", action, "CANFOCUS=NO, FLAT=YES")
--  gdx item = gCanvas(action, "CANFOCUS=NO, FLAT=YES") -- erm, no...
    gSetAttribute(item, "IMAGE", image)
    gSetAttribute(item, "USER_DATA", id)
    integer k = find('(',tip)
    if k!=0 then
        k += 1
        integer l = find(')',tip,k)-1
--      tip[k..l] = keybd:mapkeytext(tip[k..l])
    end if
    gSetAttribute(item, "TIP", tip)
--  gSetAttribute(item, "SIZE", {20,20})
    gSetAttribute(item, "SIZE", {24,24})
--  gSetAttribute(item, "SIZE", {16,16})
    gSetInt(item, "ACTIVE", active)
    return item
end function

