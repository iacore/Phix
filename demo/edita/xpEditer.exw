--/* ditty for estimating gTabs() width: (DEV where should I keep this??)
function tabw(sequence s, integer m, p, x, e)
    integer res = x+(length(s)-1)*p
    for si in s do
        res += max(si,m)
    end for
--  res = abs(res-e)
    return res
--  return e-res
end function
